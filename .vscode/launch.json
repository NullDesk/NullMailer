{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Samples (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceRoot}\\samples\\mailer-cli\\mailer\\bin\\Debug\\netcoreapp1.1\\mailer.dll",
            "args": ["send", "simple", "-a"],
            "cwd": "${workspaceRoot}\\samples\\mailer-cli\\mailer",
            "symbolPath": [],
            "externalConsole": false,
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}